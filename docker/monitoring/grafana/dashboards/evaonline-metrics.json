        "timezone": "America/Sao_Paulo",
        "schemaVersion": 38,
        "version": 2,
        "refresh": "15s",
        "panels": [
            {
                "id": 1,
                "title": "üìä API Requests por Segundo",
                "type": "graph",
                "description": "Taxa de requests por segundo para a API",
                "targets": [
                    {
                        "expr": "rate(http_requests_total[1m])",
                        "legendFormat": "{{method}} {{endpoint}} ({{status}})",
                        "refId": "A"
                    }
                ],
                "fieldConfig": {
                    "defaults": {
                        "unit": "reqps",
                        "min": 0,
                        "color": {
                            "mode": "palette-classic"
                        },
                        "custom": {
                            "axisLabel": "Requests/segundo",
                            "drawStyle": "line",
                            "lineInterpolation": "linear",
                            "lineWidth": 2,
                            "fillOpacity": 10,
                            "gradientMode": "opacity",
                            "spanNulls": false
                        }
                    }
                },
                "gridPos": {
                    "h": 8,
                    "w": 12,
                    "x": 0,
                    "y": 0
                }
            },
            {
                "id": 2,
                "title": "‚è±Ô∏è Tempo de Resposta (Percentis)",
                "type": "graph",
                "description": "Tempo de resposta em diferentes percentis",
                "targets": [
                    {
                        "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[2m]))",
                        "legendFormat": "P95",
                        "refId": "A"
                    },
                    {
                        "expr": "histogram_quantile(0.50, rate(http_request_duration_seconds_bucket[2m]))",
                        "legendFormat": "P50",
                        "refId": "B"
                    },
                    {
                        "expr": "histogram_quantile(0.99, rate(http_request_duration_seconds_bucket[2m]))",
                        "legendFormat": "P99",
                        "refId": "C"
                    }
                ],
                "fieldConfig": {
                    "defaults": {
                        "unit": "s",
                        "min": 0,
                        "decimals": 3,
                        "color": {
                            "mode": "palette-classic"
                        },
                        "custom": {
                            "axisLabel": "Segundos",
                            "drawStyle": "line",
                            "lineWidth": 2
                        },
                        "thresholds": {
                            "mode": "absolute",
                            "steps": [
                                {
                                    "value": null,
                                    "color": "green"
                                },
                                {
                                    "value": 1,
                                    "color": "yellow"
                                },
                                {
                                    "value": 2,
                                    "color": "red"
                                }
                            ]
                        }
                    }
                },
                "gridPos": {
                    "h": 8,
                    "w": 12,
                    "x": 12,
                    "y": 0
                }
            },
